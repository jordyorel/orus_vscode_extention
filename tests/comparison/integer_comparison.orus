// Test integer comparison operations
let a: i32 = 10
let b: i32 = 20
let c: i32 = 10

print("i32 Comparisons:")

print("10 == 20 is: {}", a == b)

print("10 == 10 is: {} ", a == c)

print("10 != 20 is: {} ", a != b)

print("10 < 20 is: {} ", a < b)

print("10 <= 20 is: {} ", a <= b)

print("10 <= 10 is: {} ", a <= c)

print("10 > 20 is: {} ", a > b)

print("10 >= 20 is: {}", a >= b)

print("10 >= 10 is: {} ", a >= c)



