// Test floating point comparison operations

let a: f64 = 10.5
let b: f64 = 20.7
let c: f64 = 10.5

print("f64 Comparisons:")

print("10.5 == 20.7 is: {} ", a == b)

print("10.5 == 10.5 is: {} ", a == c)

print("10.5 != 20.7 is {} ", a != b)

print("10.5 < 20.7 is: {}", a < b)

print("10.5 <= 20.7 is: {} ", a <= b)

print("10.5 <= 10.5 is: {} ", a <= c)

print("10.5 > 20.7 is: {} ", a > b)

print("10.5 >= 20.7 is: {} ", a >= b)

print("10.5 >= 10.5 is: {} ", a >= c)


